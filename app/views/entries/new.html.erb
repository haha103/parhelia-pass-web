<h1>New Entry</h1>
<%= form_for :entry, url: entries_path do |f| %>
  <p>
  <%= f.label :key %><br>
  <%= f.text_field :key %>
  </p>

  <p>
  <%= f.label :owner %><br>
  <%= f.text_field :owner %>
  </p>

  <p>
  <%= f.label :user %><br>
  <%= f.text_field :user %>
  </p>

  <p>
  <%= f.label :password %><br>
  <%= f.text_field :password %>
  </p>

  <p>
  <%= f.label :category %><br>
  <%= f.text_field :category %>
  </p>

  <p>
  <%= f.label :comment %><br>
  <%= f.text_area :comment %>
  </p>

  <p>
  <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', entries_path %>
